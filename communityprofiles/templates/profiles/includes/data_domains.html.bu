{% spaceless %}
<nav class="navbar navbar-default" role="navigation">
    <ul id="domains-wrap" class="nav navbar-nav">
{% for domain in data_domains %}
<li class="domain">
    <a href="{{domain.href}}"{% if domain.domain == selected_domain %} class="active"{% endif %}>
        {{ domain.domain.name }}
	</a>
</li>
{% endfor %}
</ul>
</nav>
{% endspaceless %}
